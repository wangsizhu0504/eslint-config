{
  "name": "@kriszu/eslint-config",
  "type": "module",
  "version": "3.2.12",
  "description": "Kriszu ESLint config",
  "author": "Kriszu <wangsizhu0504@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangsizhu0504/eslint-config.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "npm run typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "lint": "eslint .",
    "typegen": "esno scripts/typegen.ts",
    "prepack": "npm run build",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": ">=1.5.0",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=8.40.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react-hooks": ">=4.6.0",
    "eslint-plugin-react-refresh": ">=0.4.4"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@kriszu/eslint-plugin": "workspace:*",
    "@stylistic/eslint-plugin": "^2.6.0-beta.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint-flat-config-utils": "^0.2.5",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-command": "^0.2.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^3.0.1",
    "eslint-plugin-jsdoc": "^48.7.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "find-up": "^7.0.0",
    "globals": "^15.8.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.0.1",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.26",
    "@stylistic/eslint-plugin-migrate": "^2.3.0",
    "@unocss/eslint-plugin": "^0.61.3",
    "eslint-plugin-format": "^0.1.2",
    "eslint": "^9.4.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "eslint-typegen": "^0.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
