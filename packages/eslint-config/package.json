{
  "name": "@kriszu/eslint-config",
  "type": "module",
  "version": "3.4.1",
  "description": "Kriszu ESLint config",
  "author": "Kriszu <wangsizhu0504@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangsizhu0504/eslint-config.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "npm run typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "watch": "tsup --format esm --watch",
    "lint": "eslint .",
    "typegen": "esno scripts/typegen.ts",
    "prepack": "npm run build",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.19.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": "^9.10.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.4"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.10.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/markdown": "^6.3.0",
    "@kriszu/eslint-plugin": "workspace:*",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitest/eslint-plugin": "^1.1.37",
    "ansis": "^3.17.0",
    "cac": "^6.7.14",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-command": "^3.1.0",
    "eslint-plugin-import-x": "^4.8.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-pnpm-catalogs": "^0.0.3",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "find-up": "^7.0.0",
    "globals": "^15.15.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "parse-gitignore": "^2.0.0",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.30.2",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin-migrate": "^4.2.0",
    "@types/yargs": "^17.0.33",
    "@unocss/eslint-plugin": "^65.5.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-typegen": "^1.0.0"
  },
  "resolutions": {
    "@eslint-community/eslint-utils": "^4.5.1",
    "@typescript-eslint/utils": "^8.26.0",
    "eslint": "^9.22.0",
    "tsx": "^4.19.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
