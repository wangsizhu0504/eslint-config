{
  "name": "@kriszu/eslint-plugin",
  "version": "3.2.2",
  "description": "Kriszu extended ESLint rules",
  "author": "Kriszu <wangsizhu0504@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangsizhu0504/eslint-config.git",
    "directory": "packages/eslint-plugin"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.9",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/rule-tester": "^6.19.1",
    "@typescript-eslint/typescript-estree": "^6.19.1",
    "@typescript-eslint/utils": "^6.19.1",
    "ajv": "^8.12.0",
    "eslint-define-config": "^2.1.0",
    "lint-staged": "^15.2.0",
    "lodash.merge": "4.6.2",
    "semver": "^7.5.4"
  }
}
