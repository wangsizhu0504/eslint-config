{
  "name": "@kriszu/eslint-plugin",
  "type": "module",
  "version": "5.1.0-beta.1",
  "description": "Kriszu extended ESLint rules",
  "author": "Kriszu <wangsizhu0504@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangsizhu0504/eslint-config.git",
    "directory": "packages/eslint-plugin"
  },
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --clean --dts",
    "lint": "pnpm dev && eslint .",
    "prepublishOnly": "pnpm build",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "catalog:prod",
    "@typescript-eslint/utils": "catalog:peer",
    "eslint-vitest-rule-tester": "catalog:prod",
    "jsonc-eslint-parser": "catalog:prod"
  }
}
