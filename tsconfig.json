{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "ESNext",
      "dom"
    ],
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noUnusedParameters": true,
    "jsx": "preserve"
  },
  "exclude": [
    "dist/**",
    "node_modules/**"
  ]
}
