    "@types/node": "^20.12.11",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "bumpp": "^9.4.1",
    "bundle-require": "^4.1.0",
    "eslint": "9.2.0",
    "eslint-flat-config-viewer": "^0.1.20",
    "esno": "^4.7.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
